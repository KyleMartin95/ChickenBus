{
    "compilerOptions": {
        "outDir": "./dist/js/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "experimentalDecorators": true,
        "allowJs": true,
        "types": [
            "googlemaps",
            "geojson",
            "prop-types",
            "react",
            "react-dom"
        ]
    },
    "include": [
        "./src/**/*"
    ]
}
